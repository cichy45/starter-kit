<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form">
    <Dialog id="contactDialog" title="{i18n>contactDetails}">
    	<VBox id="_IDGenVBox1" alignItems="Center">
        	<f:Form id="FormDisplay354" editable="false">
			<f:title>
				<core:Title id="_IDGenTitle1" text="{i18n>contactDescription}" />
			</f:title>
			<f:layout>
				<f:ResponsiveGridLayout id="_IDGenResponsiveGridLayout1"
					labelSpanXL="3"
					labelSpanL="3"
					labelSpanM="3"
					labelSpanS="12"
					adjustLabelSpan="false"
					emptySpanXL="4"
					emptySpanL="4"
					emptySpanM="4"
					emptySpanS="0"
					columnsXL="1"
					columnsL="1"
					columnsM="1"
					singleContainerFullSize="false" />
			</f:layout>
			<f:formContainers>
				<f:FormContainer id="_IDGenFormContainer1">
					<f:formElements>
						<f:FormElement id="_IDGenFormElement1" label="Contact Name">
							<f:fields>
								<Text id="contactname" text="{ContactName}"></Text>
							</f:fields>
						</f:FormElement>
						<f:FormElement id="_IDGenFormElement2" label="Contact Title">
							<f:fields>
								<Text id="contacttitle" text="{ContactTitle}"></Text>
							</f:fields>
						</f:FormElement>
						<f:FormElement id="_IDGenFormElement3" label="Address">
							<f:fields>
								<Text id="address" text="{Address}" />
							</f:fields>
						</f:FormElement>
						<f:FormElement id="_IDGenFormElement4" label="Phone number">
							<f:fields>
								<Text id="phonenumber" text="{Phone}" />
							</f:fields>
						</f:FormElement>
					</f:formElements>
				</f:FormContainer>
			</f:formContainers>
		    </f:Form>
			<Button id="_IDGenButton1" text="Cancel" press="onCancel" />
		</VBox>
    </Dialog>
</core:FragmentDefinition>